def only_evens(list_int: list[int]) -> list:
    even_numbers = []
    for number in list:
        if number % 2 == 0: 
            even_numbers.append(number)
    return even_numbers


def concat(list_int: list[int], list_int2: list[int]) -> list:
    for number in list_int:
        list_int2.append(number)